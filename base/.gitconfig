[user]
	name = Tatsuya Kitagawa
	email = tatsuya.kitagawa@t13a.com
[ghq]
	root = ~/Documents
[gui]
	spellingdictionary = none
	fontui = -family \"Noto Sans CJK JP\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family \"Input Mono Narrow\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
[core]
	editor = vim
	quotepath = off
[alias]
	a = add
	b = branch
	c = commit
	co = checkout
	d = diff
	l = log
	m = merge
	rb = rebase
	rs = reset
	s = status
	t = tag
	y = stash
[merge]
	tool = meld
[secrets]
	patterns = ^-+BEGIN\\sRSA\\sPRIVATE\\sKEY-+$
